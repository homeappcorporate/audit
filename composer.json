        {
            "name": "Legion112",
            "email": "ageev.maks2014@gmail.com"
        }
    ],
    "require": {
        "symfony/http-kernel": "^5.0|^4.0",
        "doctrine/orm": "^2.0",
        "ramsey/uuid": "^4.0",
        "symfony/dependency-injection": "^5.3|^4.0",
        "doctrine/migrations": "^2.0",
        "symfony/config": "^5.3|^4.0"
    }
}
